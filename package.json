{
  "name": "store_backend",
  "version": "1.0.0",
  "description": "store_backed project2 for udacity powred by:FWD",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=dev && nodemon ./src/server.ts",
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "format": "prettier --config .prettierrc ./src/**/*.ts -w",
    "lint": "eslint --config .eslintrc ./src/**/*.ts",
    "test": "set NODE_ENV=test && db-migrate -e test up && npm run build && jasmine && db-migrate -e test reset",
    "migrate-up": "set NODE_ENV=dev && db-migrate up",
    "migrate-down": "set NODE_ENV=dev && db-migrate down",
    "migrate-reset": "db-migrate reset"
  },
  "author": "Ahmed-Hall",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "pg": "^8.8.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  }
}
